{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "bigiayomide/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "bigiayomide/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "bigiayomide/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "bigiayomide/multi-nginx",
            "hostname": "worker",
            "essential": true,
            "portMappings":[{
                "hostPort":80,
                "containerPort":80
            }],
            "links":[
                "client","server"
            ]
        }
    ]
}